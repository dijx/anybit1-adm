docker run -d \
--mount type=bind,source="/opt/docker/mount/nextCloud/mysql/etc",target="/etc" \
--mount type=bind,source="/opt/docker/mount/nextCloud/mysql/var_lib_mysql",target="/var/lib/mysql" \
--mount type=bind,source="/adm/BACKUP/mysql",target="/BACKUP" \
--net nextCloud-net \
--ip 172.16.1.3 \
--name nextCloud-sql \
--restart unless-stopped \
-e MYSQL_ROOT_PASSWORD=mysqlRoot123 mysql:latest \

